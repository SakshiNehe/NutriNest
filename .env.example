# Spoonacular API Key (get yours at https://spoonacular.com/food-api)
EXPO_PUBLIC_SPOONACULAR_API_KEY=your_spoonacular_api_key_here

# Firebase configuration - you can use these or your own
# Note: In a production app, you'd typically not expose Firebase config in .env
# But since these are client-side values anyway, it's acceptable for this use case
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_firebase_project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_firebase_project_id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_firebase_project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
EXPO_PUBLIC_FIREBASE_APP_ID=your_firebase_app_id
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=your_measurement_id 